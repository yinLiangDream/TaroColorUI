		"husky": "^2.4.1",
		"jest": "^24.8.0",
		"lint-staged": "^8.0.3",
		"marked": "^0.7.0",
		"nerv-devtools": "^1.5.7",
		"nerv-server": "^1.4.0",
		"nerv-test-utils": "^1.4.0",
		"nervjs": "^1.5.7",
		"node-sass": "^4.12.0",
		"qrcode": "^1.4.2",
		"regenerator-runtime": "^0.12.1",
		"rollup": "^2.3.1",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-typescript2": "^0.27.0",
		"rollup-plugin-visualizer": "^4.0.2",
		"sass-loader": "^8.0.0",
		"simulant": "^0.2.2",
		"sinon": "^7.3.2",
		"taro_rich_text": "^1.1.3",
		"tslint": "^5.19.0",
		"typescript": "^3.8.3",
		"vuepress": "^1.1.0"
	},
	"peerDependencies": {
		"@tarojs/components": ">=3",
		"@tarojs/react": ">=3",
		"react": ">=16.13.0",
		"react-dom": ">=16.13.0"
	}
}
